{
    "name": "dotkernel/dot-authorization",
    "type": "library",
    "description": "DotKernel authorization service abstractions",
    "license": "MIT",
    "authors": [
        {
            "name": "DotKernel Team",
            "email": "team@dotkernel.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "laminas/laminas-dependency-plugin": "^1.0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1.2",
        "squizlabs/php_codesniffer": "^3.5.5"
    },
    "autoload": {
        "psr-4": {
            "Dot\\Authorization\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DotTest\\Authorization\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev",
            "dev-develop": "3.1-dev"
        }
    }
}
